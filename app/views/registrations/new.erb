<h1>Sign Up</h1>

<%= form_for(@user, url: registrations_path(@user)) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.submit %>
<% end %>
